{
  "AuthServer": {
    "Authority": "http://authserver:53362",
    "ApiName": "InternalGateway"
  },
  "ConnectionStrings": {
    "Default": "Server=172.17.0.6,1433;Database=ABP;Trusted_Connection=False;User ID=sa;Password=yourStrong(!)Password;"
  },
  "ElasticSearch": {
    "Url": "http://localhost:9200"
  },
  "Redis": {
    "Configuration": "172.17.0.1"
  },
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/api/identity/{everything}",
